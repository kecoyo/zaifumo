{
  "name": "zaifumo",
  "version": "1.0.0-beta.2",
  "description": "zaifumo",
  "keywords": [
    "zaifumo",
    "typescript"
  ],
  "homepage": "https://github.com/kecoyo/zaifumo",
  "repository": "kecoyo/zaifumo",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "scripts": {
    "build:es": "tsc --module esnext --outDir ./es",
    "build:commonjs": "tsc --module commonjs --outDir ./lib",
    "build:watch": "tsup --watch",
    "build:types": "tsc -d --emitDeclarationOnly --outDir ./types --noEmit false",
    "build": "npm run build:es && npm run build:commonjs && npm run build:types",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "pub": "nrm use npm && npm run build && npm publish",
    "tsup": "tsup",
    "tsup-dts": "tsup src/index.ts --dts"
  },
  "prettier": "prettier-config-ljlx",
  "dependencies": {
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "prettier-config-ljlx": "^1.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
